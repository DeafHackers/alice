[% USE String(channel.name) %]
[% channel.name_clean = String.replace('#','chan_') %]
<li id="[% channel.name_clean %]_tab"[% IF loop.first %] class="active"[% END %] onclick="showChannel('[% channel.name_clean %]')">
  <div onclick="partChannel('[% channel.name_clean %]')" class="tab_button"></div>
  [% channel.name %]
</li>